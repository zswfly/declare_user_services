<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zsw.entitys.PermissionEntity" table="permission" schema="user">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="code">
            <column name="code" sql-type="varchar(80)" length="80"/>
        </property>
        <property name="createTime">
            <column name="create_time" sql-type="datetime"/>
        </property>
        <property name="createUser">
            <column name="create_user" sql-type="int(11)"/>
        </property>
        <property name="updateTime">
            <column name="update_time" sql-type="datetime"/>
        </property>
        <property name="updateUser">
            <column name="update_user" sql-type="int(11)"/>
        </property>
        <property name="status">
            <column name="status" sql-type="int(3)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>